        "@prisma/client": "6.14.0",
        "@radix-ui/react-dropdown-menu": "^2.1.16",
        "@radix-ui/react-popover": "^1.1.15",
        "@tailwindcss/postcss": "^4.1.12",
        "@tanstack/react-query": "^5.85.3",
        "@trpc/client": "^11.4.4",
        "@trpc/next": "^11.4.4",
        "@trpc/react-query": "^11.4.4",
        "@trpc/server": "^11.4.4",
        "class-variance-authority": "^0.7.1",
